<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<AnchorPane cacheShape="false" centerShape="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            prefHeight="300.0" prefWidth="300.0" scaleShape="false" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="battleship.controller.MainController">
    <GridPane centerShape="false" layoutX="100.0" layoutY="84.0" vgap="4.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="-Infinity" prefHeight="30.0" vgrow="NEVER"/>
            <RowConstraints minHeight="-Infinity" prefHeight="30.0" vgrow="NEVER"/>
            <RowConstraints minHeight="-Infinity" prefHeight="30.0" vgrow="NEVER"/>
            <RowConstraints minHeight="-Infinity" prefHeight="30.0" vgrow="NEVER"/>
        </rowConstraints>
        <Button mnemonicParsing="false" onAction="#loadGame" prefHeight="25.0" prefWidth="112.0" text="Load Game"
                GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="1"
                GridPane.valignment="CENTER" GridPane.vgrow="NEVER"/>
        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="112.0" text="Watch Replay"
                GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="2"
                GridPane.valignment="CENTER" GridPane.vgrow="NEVER"/>
        <Button mnemonicParsing="false" onAction="#newGame" prefHeight="25.0" prefWidth="112.0" text="New Game"
                GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.valignment="CENTER"
                GridPane.vgrow="NEVER"/>
        <Button mnemonicParsing="false" onAction="#exit" prefHeight="25.0" prefWidth="112.0" text="Exit"
                GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="3"
                GridPane.valignment="CENTER" GridPane.vgrow="NEVER"/>
    </GridPane>

</AnchorPane>
